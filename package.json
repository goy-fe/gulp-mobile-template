{
  "name": "gulp-mobile-template",
  "version": "0.0.2",
  "keywords": [
    "gulp",
    "mobile",
    "scss",
    "postcss",
    "template",
    "boilerpate",
    "imagemin"
  ],
  "description": "A mobile project boilerplate based on gulp 4.x",
  "main": "gulpfile.js",
  "license": "MIT",
  "scripts": {
    "dev": "gulp dev",
    "build": "gulp build",
    "pretty": "prettier --write **/*.{scss,js}",
    "toc": "markdown-toc -i README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "ntnyq <ntnyq13@gmail.com>",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "browser-sync": "^2.26.5",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-base64": "^0.1.3",
    "gulp-changed": "^3.2.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^2.2.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^7.0.0",
    "js-yaml": "^3.13.1",
    "lint-staged": "^8.1.6",
    "markdown-toc": "^1.2.0",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.17.0"
  },
  "dependencies": {}
}
